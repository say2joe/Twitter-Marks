{
  "manifest_version": 2,

  "name": "Twitter Marks",
  "description": "This extension creates bookmarks from Twitter favorites.",
  "version": "1.0",

  "permissions": [
    "https://api.twitter.com/*",
    "bookmarks",
    "tabs"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "page": "popup.html"
   }
}
